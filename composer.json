    "require": {
        "contao/core-bundle": "^4.13 || ^5.0"
    },
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoRecurringElementBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lukas Bableck",
            "email": "lukas.bableck@netprofit.de"
        }
    ],
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "extra": {
        "contao-manager-plugin": "Lukasbableck\\ContaoRecurringElementBundle\\ContaoManager\\Plugin"
    }
